float arith(float a) {
    return a + 2 - 3 * 4 / 5 + 1.1 - 2.1 * 10;
}

int main() {
    int a;
    write("digite o valor de \033[33m'a'\033[0m: ");

    read(a);

    write("func call: 'calc(a)' = ");
    writeln(arith(a));

    write("static int: ");
    writeln(78);

    write("static float: ");
    writeln(3.14159);

    write("static '");
    write(a);
    write(" + 1 + 65 - 7.89 * 5.2 / 0.45' = ");
    writeln(a + 1 + 65 - 7.89 * 5.2 / 0.45);

    return 0;
}