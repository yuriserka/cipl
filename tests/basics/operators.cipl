int i;
int j;

int main() {
    // Arithmetic Operators

    i = i + j;
    i = i - j;
    i = i * j;
    i = i / j;

    // Relational Operators

    i = i == j;
    i = i != j;
    i = i > j;
    i = i >= j;
    i = i < j;
    i = i <= j;

    // Logical Operators

    i = i && j;
    i = i || j;
    i = !i;
}